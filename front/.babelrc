{
  "presets": ["module:metro-react-native-babel-preset"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src/"],
        "extensions": [
          ".ios.ts",
          ".android.ts",
          ".ts",
          ".ios.tsx",
          ".android.tsx",
          ".tsx",
          ".jsx",
          ".js",
          ".json"
        ],
        "alias": {
          "@app": "./src/app",
          "@screens": "./src/screens",
          "@widgets": "./src/widgets",
          "@entities": "./src/entities",
          "@shared": "./src/shared",
          "@navigation": "./src/navigation"
        }
      }
    ]
  ]
}
