type Mutation {
    addFundAuth(authInput: AuthInput, fundInput: FundInput): Auth
    addUserAuth(authInput: AuthInput, userInput: UserInput): Auth

    addNews(newsInput: NewsInput): News
    addFees(feesInput: FeesInput): Fees
    addEvent(eventInput: EventInput): Event

    editFund(token: String!, data: FundInput): Fund
    setFundImage(token: String!, image: String!): Fund

    addTransaction(transactionInput: TransactionInput): Transaction
}