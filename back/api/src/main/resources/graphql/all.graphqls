type User {
    id: ID!
    name: String!
    surname: String!
    patronymic: String
    phone: String
    email: String
}

type News {
    id: ID!
    title: String
    text: String
}

input UserInfo {
    name: String!
    email: String!
}

type Mutation {
    addUser(info: UserInfo): User
}

type Query {
    getAllUsers: [User]!
    getAllNews: [News]!
}